{
  "name": "alfa-partners-grid",
  "private": false,
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex-lit/alfa-partners-grid.git"
  },
  "keywords": [
    "vue"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alex-lit/alfa-partners-grid/issues"
  },
  "author": "Alexey Litovchenko",
  "maintainers": [
    {
      "name": "Alexey Litovchenko",
      "email": "alex.lit@outlook.com",
      "url": "https://alexlit.gitlab.io"
    }
  ],
  "homepage": "https://github.com/alex-lit/alfa-partners-grid#readme",
  "scripts": {
    "serve": "vue-cli-service serve & chokidar './src/**/*.vue' -c \"if [ '{event}' = 'change' ]; then stylelint {path} --fix; fi;\"",
    "build": "cross-env OUTPUT_DIR=docs vue-cli-service build",
    "lint": "vue-cli-service lint",
    "eslint": "eslint --ext .js,.ts,.vue ./src --fix",
    "stylelint": "stylelint 'src/**/*.css' 'src/**/*.pcss' 'src/**/*.vue' --fix",
    "up": "npx npm-check-updates -u & npm i && npm audit fix"
  },
  "dependencies": {
    "bowser": "^2.9.0",
    "css-has-pseudo": "^1.0.0",
    "lodash": "^4.17.15",
    "nanoid": "^3.1.3",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-mq": "^1.0.1",
    "vue-property-decorator": "^9.1.2",
    "vue2-transitions": "^0.3.0"
  },
  "devDependencies": {
    "@alexlit/vue-component-source": "^1.1.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "@vue/cli-plugin-eslint": "^4.5.10",
    "@vue/cli-plugin-typescript": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "chokidar-cli": "^2.1.0",
    "commitizen": "^4.0.4",
    "cq-prolyfill": "^0.4.0",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsdoc": "^23.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-each": "^0.10.0",
    "postcss-for": "^2.1.1",
    "postcss-html": "^0.36.0",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "postcss-object-fit-images": "^1.1.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.4",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^13.3.2",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "~3.8.3",
    "vue-template-compiler": "^2.6.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "npm run eslint",
      "git add"
    ],
    "src/**/*.ts": [
      "prettier --write",
      "npm run eslint",
      "git add"
    ],
    "src/**/*.vue": [
      "prettier --write",
      "npm run eslint",
      "npm run stylelint",
      "git add"
    ],
    "src/**/*.css": [
      "npm run stylelint",
      "git add"
    ],
    "src/**/*.pcss": [
      "npm run stylelint",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  }
}
